<apex:page controller="Q9Controller" lightningStylesheets="true">
    <apex:form>
        <apex:pageBlock>
            <apex:message/>
            <apex:pageBlockSection title="数字を入力してください" collapsible="falese" columns="1">
                <apex:inputText value="{!inputVal}"/>
                <apex:inputText value="{!multi1}">倍数①</apex:inputText>
                <apex:inputText value="{!multi2}">倍数②</apex:inputText>
                <apex:pageMessages />
            </apex:pageBlockSection>
            <div align="right">
                <apex:commandButton action="{!multipleCalc}" value="計算"/>
            </div>
        </apex:pageBlock>
    </apex:form>
</apex:page>